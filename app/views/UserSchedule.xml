<Alloy>
	<Window id="winUserSchedule" backButtonTitle="">
		<Require platform="android" src="header" screenId="winUserSchedule" onBackButtonClick="nsUserSchedule.closeWindow" onSetting="nsUserSchedule.getSettings"></Require>
		<View id="vwMain">
			<View id="vwTopYellow">
				<Label id="lblYourSchedule" text="Your Schedule"></Label>
			</View>
			<View id="vwDays" class="vwDaysClass">
				<View id="vwDay1" class = "dayClass" day="friday" >
					<Label id="lblDay1" class="lblDayClass" text = "Friday"></Label>
				</View>
				<View id="vwDays2" class="vwDaysClass">
					<View id="vwDay2" class = "dayClass" day="saturday" >
						<Label id="lblDay2" class="lblDayClass" text="Saturday"></Label>
					</View>
					<View id="vwDay3" class = "dayClass" day="sunday" >
						<Label id="lblDay3" class="lblDayClass" text="Sunday"></Label>
					</View>
				</View>
				<View id="vwDay4" class = "dayClass" day="monday" >
					<Label id="lblDay4" class="lblDayClass" text="Monday"></Label>
				</View>
			</View>
			<View id="vwBandSchedule"></View>
		</View>
		<View id="vwNoSchedule">
			<Label id="lblNoSchedule"></Label>
			<View id="vwImagesNoSchedule">
				<ImageView id="ivHand"></ImageView>
				<ImageView id="ivStar"></ImageView>
			</View>
		</View>
	</Window>
</Alloy>